<?xml version="1.0"?>
<package >
  <metadata minClientVersion="2.8.1">
    <id>Plugin.PushNotification</id>
    <version>1.3.0</version>
    <title>Push Notification Plugin for Xamarin</title>
    <authors>Rendy Del Rosario</authors>
    <owners>crossgeeks</owners>
